<div class="container-fluid col-md-10 col-md-offset-1">
<% @artist.songs.each do |song| %>
<div class="col-md-3 col-md-offset-1  panel panel-primary">
<div class = "col-md-5"> <h4><%= song.name %></h4></div>
<div class="col-md-6 col-md-offset-1"><%= image_tag song.album_image.image , :class => "style_image"%></div>
<p>Track <%= song.track_number %>
of <%= song.album %>
</p>
</div>
<% end %>
</div>
